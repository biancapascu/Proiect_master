@model IEnumerable<Proiect_master.Models.ShopViewModel.OrderGroup>
@{
    ViewData["Title"] = "Ordered Instruments Statistics";
}
<h2>Ordered Instruments Statistics</h2>
<table>
    <tr>
        <th width="150">
            Order Date
        </th>
        <th width="150">
            Ordered Instruments
        </th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.OrderDate)
            </td>
            <td>
                @item.InstrumentCount
            </td>
        </tr>
    }
</table>